CREATE TABLE tb_cliente(
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	nome VARCHAR(50) NOT NULL,
	cidade VARCHAR(50) NOT NULL,
	estado VARCHAR(50) NOT NULL,
	pais VARCHAR(50) NOT NULL
);



CREATE TABLE tb_combustivel(
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	tipo VARCHAR(50) NOT NULL
);

CREATE TABLE tb_carro(
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	idCombustivel INT,
	quilometros INT NOT NULL,
	chassi VARCHAR(50) NOT NULL,
	marca VARCHAR(50) NOT NULL,
	modelo VARCHAR(50) NOT NULL,
	ano INT NOT NULL,
	
	FOREIGN KEY(idCombustivel)
		REFERENCES tb_combustivel(id)
);

CREATE TABLE tb_vendedor(
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	nome VARCHAR(50) NOT NULL,
	sexo CHAR(1) NOT NULL,
	estado VARCHAR(50) NOT NULL
);

CREATE TABLE tb_locacao(
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	idCliente INT,
	idCarro INT,
	idVendedor INT,	
	dataLocacao DATE NOT NULL,
	horaLocacao TIME NOT NULL,
	quantidadeDias INT NOT NULL,
	valorDiaria NUMERIC(10,2) NOT NULL,
	dataEntrega DATE NOT NULL,
	horaEntrega TIME NOT NULL,
	
	FOREIGN KEY(idCliente)
		REFERENCES tb_cliente(id)
		
	FOREIGN KEY(idCarro)
		REFERENCES tb_carro(id)
		
	FOREIGN KEY(idVendedor)
		REFERENCES tb_vendedor(id)
);

